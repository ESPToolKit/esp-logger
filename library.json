{
  "name": "ESPLogger",
  "version": "1.0.1",
  "description": "A Lightweight logger implementation for ESP32",
  "keywords": [
    "esp32",
    "arduino",
    "json",
    "logger",
    "http"
  ],
  "homepage": "https://github.com/ESPToolKit/esp-logger",
  "repository": {
    "type": "git",
    "url": "https://github.com/ESPToolKit/esp-logger.git"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "zekageri"
    }
  ],
  "frameworks": ["arduino"],
  "platforms": ["espressif32"],
  "dependencies": [
    {
      "name": "ESPBufferManager",
      "version": "https://github.com/ESPToolKit/esp-buffer-manager.git"
    },
    {
      "name": "ESPWorker",
      "version": "https://github.com/ESPToolKit/esp-worker.git"
    }
  ],
  "headers": ["ESPLogger.h"],
  "build": {
    "flags": [
      "-std=gnu++17"
    ]
  }
}
